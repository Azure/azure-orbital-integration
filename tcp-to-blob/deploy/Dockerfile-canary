ARG IMAGE_NAME=mcr.microsoft.com/mirror/docker/library/alpine
ARG IMAGE_TAG=3.15

FROM $IMAGE_NAME:$IMAGE_TAG

RUN apk add --update nodejs

WORKDIR /app/

COPY ./dist/bundle/tcp-to-blob-canary.js .